@model AllLibraryModels
@{

    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Cotalog</title>
    <meta charset="utf-8" />
    <meta name = "viewport" content="width-device-width"/>
</head>
<body>
    <h2>Все книги</h2>
    @foreach(var name in Model.getallauthors){
    <div>Имя: @name.fullName , Контакты: @name.contacts , Информация: @name.information</div>
    }
    @foreach (var name in Model.getallpublishers)
    {
        <div>Название: @name.name ,Адрес:  @name.address , Контакты: @name.contacts</div>
    }
    @foreach (var name in Model.getallBibliographicmaterial)
    {
        <div>Название: @name.name ,Автор:  @name.author , Издательство: @name.publisher.name</div>
    }
   
 
</body>
</html>
